@article{yoskowitz2021_IMPROVINGOPERATIONALLIFETIME,
  title = {{{IMPROVING THE OPERATIONAL LIFETIME OF THE CEBAF PHOTO- GUN BY ANODE BIASING}}},
  author = {Yoskowitz, J T and Krafft, G A and {Palacios-Serrano}, G and Wijethunga, S and Grames, J and Hansknecht, J and {Hernandez-Garcia}, C and Poelker, M and Stutzman, M and Suleiman, R},
  year = {2021},
  journal = {Proceedings of the 12th International Particle Accelerator Conference},
  volume = {IPAC2021},
  abstract = {The operating lifetime of GaAs-based photocathodes in DC high voltage electron photo-guns is dominated by the ionization rate of residual beamline gas molecules. In this work, experiments were performed to quantify the improvement in photocathode charge lifetime by biasing the photo-gun anode with a positive voltage, which repels ions generated downstream of the anode. The photocathode charge lifetime improved by almost a factor of two when the anode was biased compared to the usual grounded configuration. Simulations were performed using the particle tracking code General Particle Tracer (GPT) with a new custom element. The simulation results showed that both the number and energy of ions play a role in the pattern of QE degradation. The experiment results and conclusions supported by GPT simulations will be presented.},
